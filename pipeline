node {
	stage('Checkout') {
    		sh 'echo hello world'
    		def scmVars = checkout scm
	}
	stage('Version') {
		sh 'echo Calculate the version'
		sh 'python /var/lib/jenkins/Build/calc_maven_version.py'
		def exists = fileExists 'version.properties'
		if (exists) {
			def version = readFile 'version.propertiessssssssssssss'
			echo version
		} else {
 			currentBuild.result = 'ABORTED'
    			error('Could not determine version number')		
		}
	}
}
